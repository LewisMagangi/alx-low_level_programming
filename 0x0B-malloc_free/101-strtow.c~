#include "main.h"
#include <string.h>
#include <stdlib.h>
#include <stdio.h>

/**
  *strtow-a function that splits a string into words.
  *@str: string input
  *
  *Return: A character input
  */
char **strtow(char *str)
{
	int i, count = -1;
	char **wow;

	if (!str || !strlen(str))
		return (NULL);

	for (i = 0; str[i] != '\0'; i++)
	{
		if(str[i]==' ' || str[i]=='\n' || str[i]=='\t')
			count++;
	}

	printf("Count: %d", count);
	return (NULL);
}
